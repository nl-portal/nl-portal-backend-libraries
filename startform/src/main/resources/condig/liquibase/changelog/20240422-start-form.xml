<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="Niek Mol" id="1">
        <createTable tableName="start_form">
            <column name="id" type="UUID">
                <constraints primaryKey="true" primaryKeyName="start_form_id_PK"/>
            </column>
            <column name="form_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="type_uuid" type="UUID">
                <constraints nullable="false"/>
            </column>
            <column name="type_version" type="NUMBER">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>